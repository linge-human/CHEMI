  "calculations": {
    "stoichiometry": [
      {
        "id": "moles-mass",
        "title": "Moles to Mass",
        "formula": "m = n × M",
        "description": "Calculate mass from moles and molar mass",
        "inputs": [
          {"id": "moles", "label": "Moles (n)", "unit": "mol", "type": "number"},
          {"id": "molar_mass", "label": "Molar Mass (M)", "unit": "g/mol", "type": "text", "placeholder": "Enter value or formula (e.g., H2O)"}
        ],
        "examples": [
          {
            "inputs": {"moles": 2, "molar_mass": "H2O"},
            "result": "36.032 g",
            "explanation": "For 2 moles of H2O (MM = 18.016 g/mol)"
          }
        ]
      },
      {
        "id": "limiting-reagent",
        "title": "Limiting Reagent Analysis",
        "formula": "Compare mole ratios",
        "description": "Determine the limiting reagent in a chemical reaction",
        "inputs": [
          {"id": "reactant_a", "label": "Reactant A Formula", "type": "text"},
          {"id": "mass_a", "label": "Mass of A", "unit": "g", "type": "number"},
          {"id": "reactant_b", "label": "Reactant B Formula", "type": "text"},
          {"id": "mass_b", "label": "Mass of B", "unit": "g", "type": "number"},
          {"id": "coefficient_a", "label": "Coefficient of A", "type": "number"},
          {"id": "coefficient_b", "label": "Coefficient of B", "type": "number"}
        ]
      }
    ],
    "equilibrium": [
      {
        "id": "equilibrium-constant",
        "title": "Equilibrium Constant",
        "formula": "Kc = [C]^c[D]^d / [A]^a[B]^b",
        "description": "Calculate equilibrium constant from concentrations",
        "inputs": [
          {"id": "products", "label": "Product Concentrations", "type": "text", "placeholder": "C:0.5,D:0.8"},
          {"id": "reactants", "label": "Reactant Concentrations", "type": "text", "placeholder": "A:0.2,B:0.3"},
          {"id": "coefficients", "label": "Stoichiometric Coefficients", "type": "text", "placeholder": "a:1,b:1,c:2,d:1"}
        ]
      },
      {
        "id": "ice-table",
        "title": "ICE Table Calculator",
        "formula": "Initial - Change = Equilibrium",
        "description": "Calculate equilibrium concentrations using ICE tables",
        "inputs": [
          {"id": "initial_concs", "label": "Initial Concentrations", "type": "text"},
          {"id": "k_value", "label": "Equilibrium Constant (K)", "type": "number"},
          {"id": "reaction_quotient", "label": "Reaction Quotient (Q)", "type": "number"}
        ]
      }
    ],
    "thermodynamics": [
      {
        "id": "gibbs-energy",
        "title": "Gibbs Free Energy",
        "formula": "ΔG = ΔH - TΔS",
        "description": "Calculate Gibbs free energy change",
        "inputs": [
          {"id": "enthalpy", "label": "Enthalpy Change (ΔH)", "unit": "kJ/mol", "type": "number"},
          {"id": "entropy", "label": "Entropy Change (ΔS)", "unit": "J/mol·K", "type": "number"},
